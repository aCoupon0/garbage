{
  "expo": {
    "name": "acouponf",
    "slug": "acouponf",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "fonts": {
      "Bogart": "./assets/fonts/Bogart.ttf",
      "VintageGoods": "./assets/fonts/Vintage-Goods.otf",
      "BogartBold": "./assets/fonts/BogartBold.ttf"
    },
    "plugins": [
      "expo-font"
    ]
  }
}